-#CONFIG_PNM_ZSWAP_MODIFY_ZBUD is not set
-#CONFIG_PNM_ZSWAP_NUMA_NODE is not set
-#CONFIG_ZCXL_ACCEL_DEFAULT_PNM is not set
-#CONFIG_ZCXL_ACCEL_DEFAULT is not set
-
 CONFIG_NVMEM=y
 CONFIG_NVMEM_SYSFS=y
 # CONFIG_NVMEM_RAVE_SP_EEPROM is not set
@@ -10079,6 +10076,10 @@ CONFIG_MOST=m
 # CONFIG_HTE is not set
 CONFIG_PNM=y
 CONFIG_SLS_RESOURCE=m
+CONFIG_PNM_ZSWAP=y
+CONFIG_PNM_ZSWAP_SIM=y
+CONFIG_PNM_ZSWAP_MODIFY_ZBUD=y
+CONFIG_PNM_ZSWAP_NUMA_NODE=1
 CONFIG_IMDB_RESOURCE=m
 # CONFIG_DEV_SLS_AXDIMM is not set
 CONFIG_DEV_SLS_CXL=y
