 # end of RCU Subsystem
 
 CONFIG_BUILD_BIN2C=y
@@ -10008,13 +10012,6 @@ CONFIG_DEV_DAX=m
 CONFIG_DEV_DAX_PMEM=m
 # CONFIG_DEV_DAX_HMEM is not set
 CONFIG_DEV_DAX_KMEM=m
